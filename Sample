$(document).ready(function(){
		var rtePath = "<%=request.getContextPath()%>";
				$("#longrunTransHome").click(function() {
							
							var longRunTransHomePath = rtePath
									+ "/eventtrack/longruntrans/longRunTransHome3";
							$("#longRunTransDisplay").attr('action',
									longRunTransHomePath).submit();
						});
				$("#longrunTransExport").click(function() {
							
							var longRunTransHomePath = rtePath + "/eventtrack/longruntrans/longRunTransExport3";
							$("#form1").attr('action',longRunTransHomePath).submit();
						});
			});
</script>
