    $('#diganosisCtr').change(function() {

        

        var selected = $(this).val();

        if(selected == '0'){

        $("#counterDiaghide").hide();

        $("#one").hide();$("#two").hide();$("#three").hide();$("#four").hide();$("#five").hide();

    	$("#six").hide();$("#seven").hide();$("#eight").hide();$("#nine").hide();$("#ten").hide();

