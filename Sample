$('#subsDept').change(function() {

       

    var selected = $(this).val();

    if (selected == 'Subscriber') { 

    	$("#dependentTable").hide();

  		$("#subscriberTable").show(); 

    }

    else if (selected == 'Dependent') {

    	$("#dependentTable").show();

  		$("#subscriberTable").hide();

    }else {

$("#dependentTable").hide();

$("#subscriberTable").hide();

    }

    });

