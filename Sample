	<head>

		<%-- <title>

			<c:choose>

				<c:when test="${title ne null}"><c:out value="${title}"/></c:when>

				<c:otherwise>

					<c:set var="titleCode"><tiles:getAsString name="title"/></c:set>

					<fmt:message key="${titleCode}"/>

				</c:otherwise>

			</c:choose>

		</title> --%>

		 <tiles:importAttribute name="title" />

		<title>

			<spring:message code="${title}"></spring:message>

		</title>

		

	</head>

	<body>

	<div id="container">

		<div id="header">

			<tiles:insertAttribute name="header" />

		</div>

		<div id="bodyContent">

			<div id="menu">

				<tiles:insertAttribute name="menu" />

			</div>

			<div id="content">

				<tiles:insertAttribute name="body" />

			</div>

		</div>

		<div id="footer">

			<tiles:insertAttribute name="footer" />

		</div>

	</div>

