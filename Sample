package com.aetna.prvrte.rteintranet.copybookbean;import com.aetna.prvrte.rteintranet.util.RteIntranetUtils;public class NonIndividualData {	String entityId="1P";	String name;	public String getEntityId() {				return entityId;	}	public void setEntityId(String entityId) {				this.entityId = entityId;	}	public String getName() {				return name;	}	public void setName(String name) {				this.name = name;	}	public StringBuilder getNonIndiv(){		return new StringBuilder(getEntityId())		.append(getName())		.append(RteIntranetUtils.spaceFiller(70));	}}
