<script>
$(document).ready( function(e) {
	var relPath="<%=request.getContextPath()%>";
	$('#adasvctLookUp').click(function(){
		var adasvctLookUpPath = relPath+'/adasvct/adasvctLookUp';
		$("#adasvctForm").attr('action', adasvctLookUpPath).submit();
	});
	
	$('#adasvctAdd').click(function(){
		var adasvctAction = relPath+'/adasvct/addAdasvctForm';
		$("#adasvctForm").attr('action', adasvctAction).submit();	
	});
});
</script>
