<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean id="aefw.application" parent="aefw.applicationBase">
		<property name="configId" value="RteIntranet"/>
		<property name="configRoot" value="${environment.root}/${configId}/config"/>
	</bean>

	<bean id="aefw.propertyValues" parent="aefw.props">
		<property name="locations">
			<list>
				
			<value>${configRoot}/application.${environment.name}.properties</value>				
				
			
			</list>
		</property>
	</bean>

</beans>
